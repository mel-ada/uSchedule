<% include header %>
<% include navigation %>
<% include styleAvailabilityTable %>
<% include styleForm%>

<div id="section">

<div>
    <h2>Employee Availability </h2>
</div>

<style>
#sep {
    height: 2px;
    background: #D1F0FF;
    width: 1300px;
    margin: 0px -25px;
}
</style>

<div id="sep"> </div>
<br>
<br>


                        <tr>
                            <% if(rs[j].a_Day == rs[k].a_Day) { %>
                                <td>
                                    <%= formatTime(rs[j].a_StartTime) %> -
                                    <%= formatTime(rs[j].a_EndTime) %>
                                    <input type="checkbox" id="e_SSN" name="e_SSN">
                                </td>
                            <% } %>
                        </tr>
                    <% } %>
                <% } %>



	    <tr>
                <% for (var i = 4; i < 5 && i > 3; i++) { %>
                    <th><%=rs[i].a_Day %></th>
                <% }%>
            </tr>

                <% for (var j = 0; j < rs.length; j++) { %>
                    <% for (var k = 4; k < 5 && k > 3; k++) { %>
                        <tr>
                            <% if(rs[j].a_Day == rs[k].a_Day) { %>
                                <td>
                                    <%= formatTime(rs[j].a_StartTime) %> -
                                    <%= formatTime(rs[j].a_EndTime) %>
                                    <input type="checkbox" id="e_SSN" name="e_SSN">
                                </td>
                            <% } %>
                        </tr>
                    <% } %>
                <% } %>


	    <tr>
                <% for (var i = 5; i < 6 && i > 4; i++) { %>
                    <th><%=rs[i].a_Day %></th>
                <% }%>
            </tr>

                <% for (var j = 0; j < rs.length; j++) { %>
                    <% for (var k = 5; k < 6 && k > 4; k++) { %>
                        <tr>
                            <% if(rs[j].a_Day == rs[k].a_Day) { %>
                                <td>
                                    <%= formatTime(rs[j].a_StartTime) %> -
                                    <%= formatTime(rs[j].a_EndTime) %>
                                    <input type="checkbox" id="e_SSN" name="e_SSN">
                                </td>
                            <% } %>
                        </tr>
                    <% } %>
                <% } %>



	    <tr>
                <% for (var i = 6; i < 7 && i > 5; i++) { %>
                    <th><%=rs[i].a_Day %></th>
                <% }%>
            </tr>

                <% for (var j = 0; j < rs.length; j++) { %>
                    <% for (var k = 6; k < 7 && k > 5; k++) { %>
                        <tr>
                            <% if(rs[j].a_Day == rs[k].a_Day) { %>
                                <td>
                                    <%= formatTime(rs[j].a_StartTime) %> -
                                    <%= formatTime(rs[j].a_EndTime) %>
                                    <input type="checkbox" id="e_SSN" name="e_SSN">
                                </td>
                            <% } %>
                        </tr>
                    <% } %>
                <% } %>


<% } else { %>
	Manager has not entered any shifts to work.
<% } %>

</table>
    <br>
    <input type="submit" value="Add Employee">
    <input type="reset" value="Clear Form">
</form>
</div>


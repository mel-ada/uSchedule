    <th>Day</th>
    <th>In</th>		
    <th>Out</th>
    <th>Station</th>
  </tr>
  
  <% for (var i=0; rs.length > i; i++) { %>
            <tr>
             <td> <%= rs[i].sh_Day %> </td>
	     <td> <%= rs[i].sh_startTime %> </td>
	     <td> <%= rs[i].sh_endTime %> </td>
	     <td> <%= rs[i].sh_stnName %> </td>
            </tr>
        <% } %>
</table>
  <% } else { %>
	No shifts have been scheduled for this employee.
  <% } %>


